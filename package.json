{
	"name": "school",
	"version": "1.0.0",
	"description": "School classes manager.",
	"main": "index.js",
	"scripts": {
		"start": "node index.js",
		"start:dev": "npx nodemon index.js",
		"db:start": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate",
		"db:create": "npx sequelize-cli db:create",
		"db:migrate": "npx sequelize-cli db:migrate",
		"db:seed": "npx sequelize-cli db:seed:all",
		"db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonathan-sapatieri/school.git"
	},
	"keywords": [
		"school",
		"classes"
	],
	"author": "Jonathan Sapatieri",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/jonathan-sapatieri/school/issues"
	},
	"homepage": "https://github.com/jonathan-sapatieri/school#readme",
	"dependencies": {
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"pg": "^8.7.1",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.6.5"
	},
	"devDependencies": {
		"nodemon": "^2.0.13",
		"sequelize-cli": "^5.0.0"
	}
}
